{
  "name": "ShopsApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start-r": "react-native start --reset-cache",
    "g-c-run": "cd android && ./gradlew clean && cd .. && react-native run-android",
    "g-c-test": "cd android && ./gradlew clean && cd .. && react-native run-android --variant=release",
    "g-c-build": "cd android && ./gradlew clean && ./gradlew app:assembleRelease",
    "g-c-bundle": "cd android && ./gradlew clean && ./gradlew bundleRelease",
    "push": "git status && git add . && git commit -m 'finish new theme' && git push origin master",
    "xcode": "xed -b ios",
    "start-port": "react-native start --port 8080",
    "android-port": "react-native run-android --port 8080 --deviceId 0151531S40118D97",
    "remove-app": "adb uninstall 'com.tasawk.tumer'",
    "r-run": "adb uninstall 'com.tasawk.tumer' && react-native run-android",
    "pod": "cd ios && pod install && cd ..",
    "splash": "react-native generate-bootsplash src/assets/main/logo-title.png --background-color=FFFFFF --logo-width=200 --assets-path=assets  --flavor=main"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/blur": "^4.2.0",
    "@react-native-community/datetimepicker": "^6.3.4",
    "@react-native-firebase/app": "^15.6.0",
    "@react-native-firebase/messaging": "^15.6.0",
    "@react-native-masked-view/masked-view": "^0.2.7",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.1",
    "@types/react-native-vector-icons": "^6.4.12",
    "axios": "^0.27.2",
    "i18next": "^21.9.2",
    "lottie-ios": "^3.4.3",
    "lottie-react-native": "^5.1.4",
    "qs": "^6.11.0",
    "query-string": "^7.1.1",
    "react": "18.1.0",
    "react-i18next": "^11.18.6",
    "react-native": "0.70.1",
    "react-native-device-info": "^10.2.0",
    "react-native-flash-message": "^0.3.1",
    "react-native-geolocation-service": "^5.3.0",
    "react-native-gesture-handler": "^2.6.1",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-maps": "^1.3.1",
    "react-native-modal-datetime-picker": "^14.0.0",
    "react-native-render-html": "^6.3.4",
    "react-native-restart": "^0.0.24",
    "react-native-safe-area-context": "^4.3.4",
    "react-native-screens": "^3.17.0",
    "react-native-share": "^7.9.0",
    "react-native-svg": "^13.2.0",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^11.23.1",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "uri-scheme": "^1.0.120"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.70.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.72.1",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
